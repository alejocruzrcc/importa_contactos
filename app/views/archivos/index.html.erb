<h1>Listado de Archivos importados</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
      <th scope="col">Estado</th>
    </tr>
  </thead>
  <tbody>
  <% @archivos.each do |archivo| %>
    <tr>
      <th scope="row"><%= archivo.id %></th>
      <td><%= archivo.nombre if archivo.nombre%></td>
      <td><%= archivo.estado if archivo.estado %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate @archivos %>