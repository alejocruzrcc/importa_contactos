<h1>Listado de Contactos No importados con Ã©xito</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Nombre</th>
      <th scope="col">Errores</th>
      <th scope="col">Archivo</th>
    </tr>
  </thead>
  <tbody>
  <% @contactosfallidos.each do |contacto| %>
    <tr>
      <th scope="row"><%= contacto.id %></th>
      <td><%= contacto.nombre if contacto.nombre%></td>
      <td><%= contacto.errores if contacto.errores %></td>
      <td><%= contacto.archivo.nombre if contacto.archivo %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate @contactosfallidos %>